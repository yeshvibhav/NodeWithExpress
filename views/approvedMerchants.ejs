<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<html>
<head>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="import" id="template-file" href="layout.html">

  <script type="text/javascript">
    window.location.hash=no-back-button;
    window.location.hash=Again-No-back-button;//again because google chrome don't insert first hash into history
    window.onhashchange=function(){
      window.location.hash=no-back-button;
      window.location.hash = no-back-button;}
  </script>
  <script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>



  <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.11/css/jquery.dataTables.min.css"/>
  <script src="https://cdn.datatables.net/1.10.11/js/jquery.dataTables.min.js"></script>
  <script>

    $(document).ready(function() {

      $('#example').DataTable();
    });
  </script>
  <style>
    body {
      font: 16px/1.4 Georgia, Serif;
      background-size: cover;
      background: #E3E5E5;
      font-family: Montserrat;
    }

    p {
      margin: 20px 0;
    }
    .block button {
      margin-top:15px ;
      width: 10%;
      height: 30px;
      background: #ffae45;
      box-sizing: border-box;
      border-radius: 5px;
      border: 1px solid #ffae45;
      color: #fff;
      font-weight: bold;
      text-transform: uppercase;
      font-size: 14px;
      font-family: Montserrat;
      outline: none;
      cursor: pointer;
    }
    /*
      Generic Styling, for Desktops/Laptops
      */
    table {

      border-collapse: collapse;
    }
    /* Zebra striping */

    th {
      background:#ffae45;
      color: white;
      font-weight: bold;
    }
    td, th {
      padding: 6px;
      border: 1px solid #c0c0c0;
      text-align: left;
    }


    .menuHeader{
      margin-top: 5%;
      margin-left: 40%;
      font-size: 30px;
      position: relative;
      display: inline-block;
      color:#ffae45;

    }
    .block{
      margin-top: 21px;
      margin-left: 160px;
    }

    .block button:hover {
      background: #ffae45;
    }
    #example_length{
      margin-left: 200px;
      color: #666;
    }
    #example_filter{
      margin-right: 200px;
      color: #666;

    }
    #example_info{
      margin-left: 200px;
      color: #666;
    }
    #example_paginate {
      margin-right: 200px;
      color: #666;
    }
    html {
      height: 100%;
    }
    body {
      min-height: 100%;
      position: relative;
    }
    body:after {
      content: '';
      display: block;
      height: 100px;
    }
    table.dataTable tr.odd { background-color:  #D3D3D3 }   /* tr. not tr: */
    table.dataTable tr.even { background-color: #f2f2f2}
  </style>
</head>
<body>
<div>  <% include ./partials/logoutPanel.ejs %></div>
  <div class="menuHeader">
    LIST  OF  USERS
  </div>
  <div>  <% include ./partials/menuBar.ejs %></div>

  <div class="block">
  <form role="form" >

    <table id="example" class="table table-striped table-bordered" cellspacing="0" width="70%">
      <thead>
      <tr>
        <th> Id    </th>
        <th> LoginId </th>
          <th> First Name </th>
          <th> Last Name </th>
          <th> Nationality </th>
          <th> Mobile Contact Number </th>
        <th> Gender </th>
        <th> Creation Date </th>
      </tr>
      </thead>
      <tbody>
      <% if(MerchantsParams.length >0){
       for(var i=0; i<MerchantsParams.length; i++) { %>
      <tr>
        <td>
          <%=MerchantsParams[i].ID %>
        </td>
        <td>
          <%=MerchantsParams[i].LOGINID %>
        </td>
          <td>
              <%=MerchantsParams[i].FIRSTNAME %>
          </td>
          <td>
              <%=MerchantsParams[i].LASTNAME%>
          </td>
          <td>
              <%=MerchantsParams[i].NATIONALITY%>
          </td>
          <td>
              <%=MerchantsParams[i].MOBILECONTACTNUMBER%>
          </td>
        <td>
          <%=MerchantsParams[i].GENDER%>
        </td>

        <td>
          <%=MerchantsParams[i].CREATIONDATE%>
        </td>
      </tr>

     <%  }
         }
      else{ %>
      <tr>
      <td>
       No Records Found
      </td>
        <td>

        </td>
        <td>

        </td>
        <td>

        </td>
      </tr>

     <% }%>
      </tbody>
    </table>
  </form>
  <script>
    window.location.hash="no-back-button";
    window.location.hash="Again-No-back-button";//again because google chrome don't insert first hash into history
    window.onhashchange=function(){window.location.hash="no-back-button";}
  </script>
</div>
<div>  <% include ./partials/footer.ejs %></div>
</body>
</html>
